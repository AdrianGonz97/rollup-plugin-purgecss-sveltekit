{
	"name": "vite-plugin-svelte-purgecss",
	"version": "0.0.2",
	"description": "Vite plugin for PurgeCSS for Svelte",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"url": "https://github.com/AdrianGonz97/vite-plugin-svelte-purgecss",
		"type": "git"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"dev": "tsup --watch",
		"build": "tsup",
		"lint": "tsc",
		"release": "pnpm run build && changeset publish"
	},
	"keywords": [
		"purgeCSS",
		"sveltekit",
		"svelte",
		"rollup",
		"tailwind",
		"vite",
		"skeleton"
	],
	"author": "AdrianGonz97",
	"license": "MIT",
	"dependencies": {
		"htmlparser2": "^8.0.1",
		"purgecss": "^5.0.0",
		"svelte-preprocess": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@types/node": "^18.11.18",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"vite": "^4.1.1",
		"vitest": "^0.28.4"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"vite": "^4.1.1"
	}
}
